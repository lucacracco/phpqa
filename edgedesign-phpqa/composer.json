{
  "name": "lucacracco/phpqa-phpqa",
  "authors": [
    {
      "name": "Luca Cracco",
      "email": "craccoluca@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.3.0",
    "cweagans/composer-patches": "~1.0",
    "dealerdirect/phpcodesniffer-composer-installer": "v0.7.1",
    "drupal/coder": "8.3.12",
    "edgedesign/phpqa": "dev-master",
    "friendsofphp/php-cs-fixer": "v2.17.3",
    "jakub-onderka/php-console-highlighter": "v0.4",
    "jakub-onderka/php-parallel-lint": "v1.0.0",
    "mglaman/phpstan-drupal": "0.12.6",
    "nette/neon": "v3.2.1",
    "pdepend/pdepend": "2.8.0",
    "phploc/phploc": "6.0.2",
    "phpmd/phpmd": "2.9.1",
    "phpmetrics/phpmetrics": "v2.7.4",
    "phpstan/phpstan": "0.12.42",
    "phpstan/phpstan-deprecation-rules": "0.12.5",
    "psalm/plugin-symfony": "^2.1",
    "sebastian/phpcpd": "5.0.2",
    "sensiolabs/security-checker": "v6.0.0",
    "squizlabs/php_codesniffer": "3.5.8",
    "vimeo/psalm": "4.3.2"
  },
  "config": {
    "vendor-dir": "vendor",
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "extra": {
    "patches": {
      "edgedesign/phpqa": {
        "Compatiblity Composer2.0": "composer2.patch",
        "Add an analizer for https://github.com/fabpot/local-php-security-checker": "security-checker.patch"
      }
    }
  }
}
